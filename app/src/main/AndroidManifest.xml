            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.KnowlegeListActivity"/>
        <activity android:name=".activity.XsqMainActivity"/>
        <activity android:name=".activity.KnowleageDetailActivty"/>
        <activity android:name=".mvp.osc.OscMainActivity"/>
